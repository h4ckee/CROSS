SYS_UID_MIN		1
SYS_UID_MAX		99
SYS_GID_MIN		1
SYS_GID_MAX		99
UID_MIN			100
UID_MAX			60000
GID_MIN			100
GID_MAX			60000
LOGIN_RETRIES		5
LOGIN_TIMEOUT		60
CHFN_AUTH		yes
CHFN_RESTRICT		rwh
ENCRYPT_METHOD		MD5
