#
# /etc/login.defs: login configuration, see login.defs(5)
#

FAIL_DELAY		0
FAILLOG_ENAB		yes
LOG_UNKFAIL_ENAB	no
LOG_OK_LOGINS		no
LASTLOG_ENAB		yes
MAIL_CHECK_ENAB		no
OBSCURE_CHECKS_ENAB	yes
QUOTAS_ENAB		no
SYSLOG_SU_ENAB		yes
SYSLOG_SG_ENAB		yes
CONSOLE			/etc/securetty
MOTD_FILE		/etc/motd
